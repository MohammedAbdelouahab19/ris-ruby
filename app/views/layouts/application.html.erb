<!DOCTYPE html>
<html lang="en">
<head>
    <% render 'layouts/head' %>
</head>
<body>
  <!-- Navbar -->
  <%= render 'layouts/navbar' %>

  <!-- Main Container -->
  <div class="container-fluid mt-4">
    <div class="row">
      <!-- Sidebar -->
      <div class="col-md-3 col-lg-2">
        <%= render 'layouts/sidebar' %>
      </div>

      <!-- Main Content Area -->
      <div class="col-md-9 col-lg-10">
        <div class="card shadow-sm rounded-3">
          <div class="card-body">
            <%= render partial: "layouts/main" %>
          </div>
        </div>
      </div>
    </div>
  </div>
</body>


</html>