<div class="card">
  <%= paginate @services, remote: true %>
</div>

<table class="table">
    <thead>
        <tr>
            <th>id</th>
            <th>name</th>
            </tr>
        </thead>
        <tbody>
            <% @services.each do |service| %>
                <tr>
                    <td><%= service.id %></td>
                    <td><%= service.name %></td>
                </tr>
            <% end %>
            </tbody>
    </table>
